{
  "name": "khatris",
  "version": "0.1.0",
  "private": true,
  "author": "Pavel Khabusov",
  "main": "main.js",
  "scripts": {
    "lint": "standard './*.js' './src/**/*.js'",
    "fix": "standard --fix './*.js' './src/**/*.js'",
    "coverage": "nyc npm run test",
    "build": "webpack --config webpack.config.babel.js --env production",
    "watch": "webpack-dev-server --open --env development",
    "start": "npm run lint && npm run test && npm run watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "ava": "^1.4.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "standard": "^12.0.1",
    "webpack": "^4.30.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-stream": "^5.2.1"
  },
  "optionalDependencies": {
    "nyc": "^14.1.0",
    "yarn": "^1.15.2"
  }
}
